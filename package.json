{
  "name": "qr-generator-styled",
  "version": "1.0.0",
  "description": "A professional QR code generator library with advanced styling options including gradients, rounded modules, and logo support",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "qr-styled": "./dist/cli/index.js",
    "qrgen": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "tsx src/cli/index.ts",
    "dev": "tsx src/cli/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "example:basic": "tsx examples/basic.ts",
    "example:programmatic": "tsx examples/programmatic.ts"
  },
  "keywords": [
    "qr",
    "qrcode",
    "qr-code",
    "generator",
    "styled",
    "gradient",
    "logo",
    "custom",
    "canvas",
    "image"
  ],
  "author": "Luis Manuel Yerena Sosa",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Luisma92/qr-generator-styled.git"
  },
  "bugs": {
    "url": "https://github.com/Luisma92/qr-generator-styled/issues"
  },
  "homepage": "https://github.com/Luisma92/qr-generator-styled#readme",
  "dependencies": {
    "canvas": "^3.2.0",
    "chalk": "^5.6.2",
    "qrcode": "^1.5.4",
    "yargs": "^18.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/yargs": "^17.0.35",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
