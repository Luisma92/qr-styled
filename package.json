{
  "name": "@qr-styled/monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "QR Code generator with styling support - Monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "clean": "npm run clean --workspaces",
    "node:build": "npm run build -w @qr-styled/node",
    "node:test": "npm run test -w @qr-styled/node",
    "browser:build": "npm run build -w @qr-styled/browser",
    "browser:test": "npm run test -w @qr-styled/browser"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luisma92/qr-styled.git"
  },
